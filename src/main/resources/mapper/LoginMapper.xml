<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huang.mapper.LoginMapper">

    <select id="login" resultType="UserDTO">
      select  username,password  from yunchun.sys_user where username=#{username} and password=#{password}

    </select>
</mapper>